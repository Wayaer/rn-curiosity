{
  "name": "rn-curiosity",
  "version": "2.4.3",
  "description": "React Native rn-curiosity API for iOS & Android",
  "main": "index.js",
  "homepage": "https://github.com/Wayaer/rn-curiosity",
  "license": "license",
  "author": "Wayaer",
  "scripts": {
    "android755": "chmod 755 android/gradlew",
    "androidBundle": "react-native bundle --platform android --entry-file index.js --reset-cache --bundle-output android/bundle/index.bundle --dev false --assets-dest android/bundle/",
    "iosBundle": "react-native bundle --platform ios --entry-file index.js --reset-cache --bundle-output ios/bundle/index.bundle --dev false --assets-dest ios/bundle/",
    "RNLink": "react-native link",
    "upgradeRN": "react-native-git-upgrade",
    "adbConnect": "adb connect 192.168.0.145:5555",
    "adbKill": "adb kill-server",
    "adbStart": "adb start-server",
    "adbTcpip": "adb tcpip 5555",
    "adbDevices": "adb devices",
    "npmPublish": "npm publish",
    "npmUnPublish": "npm unpublish rn-curiosity@2.4.2",
    "npmDeprecate": "npm deprecate rn-curiosity@2.3.0 Please update the version for rn-curiosity"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Wayaer/rn-curiosity"
  },
  "files": [
    "/android",
    "!/android/build",
    "/ios",
    "/src",
    "/*.podspec"
  ],
  "keywords": [
    "react-native"
  ],
  "dependencies": {
    "eslint": "^6.1.0",
    "prop-types": "^15.7.2",
    "rn-fetch-blob": "^0.10.16",
    "teaset": "^0.6.3"
  },
  "devDependencies": {
    "@react-native-community/eslint-config": "^0.0.5"
  },
  "peerDependencies": {
    "react-navigation": ">=3.3.0",
    "rn-fetch-blob": ">=0.10.16",
    "@react-native-community/async-storage": ">=1.5.1",
    "@react-native-community/netinfo": ">=4.1.3"
  },
  "prettier": {
    "requirePragma": true,
    "proseWrap": "always",
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "semi": true,
    "trailingComma": "all",
    "bracketSpacing": false,
    "parser": "flow",
    "jsxBracketSameLine": true
  },
  "eslintConfig": {
    "extends": "@react-native-community"
  }
}
